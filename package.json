  "scripts": {
    "dev": "USE_VITE=true NODE_ENV=development NODE_OPTIONS='--max-old-space-size=4096 --dns-result-order=ipv4first' tsx --watch server/bootstrap.ts",
    "dev:no-watch": "USE_VITE=true NODE_ENV=development NODE_OPTIONS='--max-old-space-size=4096 --dns-result-order=ipv4first' tsx server/bootstrap.ts",
    "build": "vite build && esbuild server/bootstrap.ts --platform=node --packages=external --bundle --format=esm --outdir=dist && esbuild server/workers/monte-carlo.worker.ts --platform=node --packages=external --bundle --format=esm --outdir=dist/workers",
    "start": "NODE_OPTIONS='--dns-result-order=ipv4first' node dist/bootstrap.js",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "db:apply-advisor": "tsx server/apply-advisor-migration.ts",
    "db:sync": "npm run db:push",
    "db:validate": "tsx server/validate-database-schema.ts",
    "db:gen": "drizzle-kit generate",
    "db:introspect": "drizzle-kit introspect",
    "db:studio": "drizzle-kit studio",
    "db:drift": "./scripts/check-schema-drift.sh",
    "db:fix": "npm run db:sync && npm run db:validate",
    "db:fix-final": "tsx server/fix-database-final.ts",
    "db:fix-all": "npm run db:fix-final && npm run db:validate",
    "db:fix-missing-errors": "tsx server/fix-missing-db-errors.ts",
    "db:fix-now": "tsx server/fix-all-database-issues-now.ts",
    "db:guard": "npm run db:validate && npm run db:push && npm run db:validate",
    "db:apply-sql": "tsx server/run-sql-file.ts",
    "health:all": "tsx server/health-check.ts",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:insights": "NODE_OPTIONS=--experimental-vm-modules jest server/__tests__/canonical-metrics.test.ts server/__tests__/plaid-taxable-filter.test.ts server/__tests__/prompt-builder.test.ts",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "test:coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "lint:no-math-random": "tsx scripts/check-no-math-random.ts",
    "verify:repro": "tsx test-determinism-mc.ts && tsx test-parallel-vs-sequential.ts",
    "verify:variance": "tsx test-lhs-variance.ts",
    "ci:verify": "npm run lint:no-math-random && npm run verify:repro",
    "build:state-tax-json": "tsx scripts/build-state-tax-json.ts"
  },
